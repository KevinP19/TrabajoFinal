	printf("DESCUENTOS\n");
	rp.d.IESSPersonal = rp.i.SueldoNeto * 0.0945;
	printf("IESS Personal: %.2f\n",rp.d.IESSPersonal);
	rp.d.PrestamoEmpresa = rp.i.SueldoNeto * 0;
